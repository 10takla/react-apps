{
  "name": "react-apps",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "predeploy": "vite build --mode deploy",
    "deploy": "gh-pages -d dist",
    "create": "ts-node scripts/create/index.ts",
    "start:parse": "concurrently \"ts-node ./scripts/parsingWords/index.ts\" \"npm run start:dev:server\"",
    "start": "webpack serve --env port=3000",
    "start:vite": "vite",
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack  --env mode=production apiUrl=https://production-project-server.vercel.app",
    "build:dev": "webpack  --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test": "jest --config ./config/jest/jest.config.ts",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "build-storybook -c ./config/storybook",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "postinstall": "node ./scripts/clear-cache.js",
    "build-storybook": "storybook build"
  },
